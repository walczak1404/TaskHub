<div class="calendar-container @ViewBag.CalendarClass">
    <div class="calendar__header">
        <p class="current-date"></p>
        <div class="icons">
            <span id="prev" class="material-symbols-rounded"><i class="fa-solid fa-circle-chevron-left"></i></span>
            <span id="next" class="material-symbols-rounded"><i class="fa-solid fa-circle-chevron-right"></i></span>
        </div>
    </div>
    <div class="calendar__content">
        <ul class="weeks">
            <li>Mon</li>
            <li>Tue</li>
            <li>Wed</li>
            <li>Thu</li>
            <li>Fri</li>
            <li>Sat</li>
            <li>Sun</li>
        </ul>
        <ul class="days" data-selected-date="@ViewBag.SelectedDate"></ul>
    </div>
</div>